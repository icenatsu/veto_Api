{
  "name": "veto-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development -- pnpm devdb && npx prisma generate && pnpm migrate:postgres && next dev -p 3009",
    "devdb": "dotenv -e .env.docker -- docker-compose -f docker-compose.dev.yml up -d db-dev",
    "studio": "dotenv -e .env.development -- npx prisma studio",
    "dev:app": "dotenv -e .env.docker -- next dev -p 3000",
    "stop:docker": "docker-compose -f docker-compose.dev.yml down",
    "start:docker": "docker-compose -f docker-compose.dev.yml up -d && pnpm dev:app && pnpm migrate:docker",
    "migrate:postgres": "dotenv -e .env.development -- npx prisma migrate deploy",
    "migrate:docker": "dotenv -e .env.docker -- docker-compose -f docker-compose.dev.yml run --rm app npx prisma migrate deploy",
    "build": "NEXT_DISABLE_SYMLINKS=true prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "docker": "node scripts/start.mjs",
    "prod": "node server.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.2",
    "@hookform/resolvers": "^3.9.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/bundle-analyzer": "^14.2.11",
    "@prisma/client": "^5.21.1",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-query-devtools": "^5.56.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.436.0",
    "next": "14.2.11",
    "next-auth": "5.0.0-beta.21",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-icons": "^5.3.0",
    "react-leaflet": "^4.2.1",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.12",
    "@types/node": "^20.16.5",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.11",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "postcss": "^8.4.47",
    "prisma": "^5.21.1",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.6.2",
    "webpack": "^5.94.0"
  },
  "packageManager": "pnpm@9.11.0+sha512.0a203ffaed5a3f63242cd064c8fb5892366c103e328079318f78062f24ea8c9d50bc6a47aa3567cabefd824d170e78fa2745ed1f16b132e16436146b7688f19b"
}
